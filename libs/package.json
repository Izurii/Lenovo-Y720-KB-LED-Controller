{
	"engines": {
		"node": "=16.9.1"
	},
	"dependencies": {
		"node-addon-api": "https://github.com/nodejs/node-addon-api/",
		"node-gyp": "^8.4.1"
	},
	"scripts": {
		"build": "./build.sh",
		"build-hotkey-addon": "node-gyp rebuild -C ./hotkey --target=1.2.3 --arch=x64 --dist-url=https://electronjs.org/headers",
		"build-led-addon": "node-gyp rebuild -C ./led --target=1.2.3 --arch=x64 --dist-url=https://electronjs.org/headers",
		"build-all": "yarn run build-hotkey-addon && yarn run build-led-addon",
		"clean": "cd ./led && node-gyp clean && cd ../ && cd ./hotkey && node-gyp clean"
	},
	"gypfile": true,
	"name": "addons",
	"version": "1.0.0",
	"author": "",
	"license": "ISC",
	"description": ""
}